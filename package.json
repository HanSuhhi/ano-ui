{
  "name": "@ano-ui/monorepo",
  "version": "0.6.10",
  "private": true,
  "packageManager": "pnpm@8.1.1",
  "scripts": {
    "dev": "pnpm stub",
    "build": "rimraf packages/*/dist && pnpm -r --filter=./packages/* run build && pnpm -r run build-post",
    "play:mp-weixin": "npm -C playground run dev:mp-weixin",
    "build:mp-weixin": "pnpm build && npm -C playground run build:mp-weixin",
    "play:h5": "npm -C playground run dev:h5",
    "build:h5": "pnpm build && npm -C playground run build:h5",
    "play:app": "npm -C playground run dev:app",
    "build:app": "pnpm build && npm -C playground run build:app",
    "release": "bumpp package.json packages/*/package.json",
    "stub": "pnpm -r --filter=./packages/* --parallel run stub",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@unocss/eslint-config": "^0.50.8",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "esno": "^0.16.3",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.0",
    "unocss": "^0.50.8",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  }
}
